<Window x:Class="ProjectPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectPanel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button x:Name="api" Width="180" Height="45" Margin="0 0 0 10" PreviewMouseDown="Button_PreviewMouseDown" >API</Button>
        <Button x:Name="webui" Width="180" Height="45" Margin="0 0 0 10" PreviewMouseDown="Button_PreviewMouseDown">WebUI</Button>
        <Button x:Name="cdn" Width="180" Height="45" Margin="0 0 0 10" PreviewMouseDown="Button_PreviewMouseDown">CDN</Button>
        <Button x:Name="hls" Width="180" Height="45" Margin="0 0 0 10" PreviewMouseDown="Button_PreviewMouseDown">hls</Button>
    </StackPanel>
</Window>
